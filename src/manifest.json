{
    "manifest_version": 2,
    "name": "dysl-ex",
    "short_name": "dysl-ex",
    "version": "0.0.1",
    "description": "PomoÄ‡ni extension za osobe s disleksijom",
    "permissions": [
        "tabs",
        "activeTab",
        "contextMenus",
        "http://*/*",
        "https://*/*",
        "storage",
        "webNavigation"
    ],
    "background": {
        "page": "html/background.html"
    },
    "browser_action": {
        "default_title": "dysl-ex",
        "default_icon": "assets/img/icon32.png"
    },
    "web_accessible_resources": [
        "js/*"
    ]
}
